[user]
	name = Ilya Sivakov
	email = ilyasivakov@gmail.com

[core]
	editor = code --wait

[init]
	defaultBranch = main

[color]
    ui = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
    last = cat-file commit HEAD
    st = status
    ci = commit
    co = checkout
    br = branch
    df = diff
    l  = log
